{
  "name": "@accdavcarrier/ngx-microsoft-bot-framework",
  "version": "15.1.5",
  "description": "Microsoft Bot Framework for Angular implementation with webchat via the directline api (forked from xtianus79). Compatible with Angular 15",
  "keywords": [
    "Angular",
    "Angular 15",
    "microsoft bot framework",
    "azure-bot-services",
    "adaptive-cards",
    "chat",
    "bot",
    "bot-framework",
    "conversational-ai",
    "conversational-ui",
    "webchat",
    "chatbot",
    "directline"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/accdavcarrier/ngx-microsoft-bot-framework"
  },
  "homepage": "git+https://github.com/accdavcarrier/ngx-microsoft-bot-framework.git",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build_lib": "ng build NgxMicrosoftBotFramework --configuration production",
    "copy-license": "copy .\\LICENSE .\\dist\\ngx-microsoft-bot-framework",
    "copy-readme": "copy .\\README.md .\\dist\\ngx-microsoft-bot-framework",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "npm_pack": "cd dist/ngx-microsoft-bot-framework && npm pack",
    "package": "npm run build_lib && npm run copy-files && npm run npm_pack",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~15.1.5",
